# Copy to .env and adjust. Do not commit .env with real secrets.

# Backend - Flask
FLASK_PORT=5000
FLASK_DEBUG=true
FLASK_HOST=0.0.0.0

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/property_market

# Secrets (required in production)
SECRET_KEY=your-secret-key
JWT_SECRET_KEY=your-jwt-secret-key

# CORS - comma-separated origins
CORS_ORIGINS=http://localhost:5500,http://127.0.0.1:5500,http://localhost:3000,http://127.0.0.1:3000

# Frontend (optional; set via meta tag or config.js for static build)
# FRONTEND_API_BASE=http://localhost:5000/api

# For file uploads (when you implement Cloudinary)
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret